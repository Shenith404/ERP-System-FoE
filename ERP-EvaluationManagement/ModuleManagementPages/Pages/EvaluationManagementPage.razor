
<MudPaper>
    <MudTabs Outlined="true" Rounded="true" Border="true"
             ApplyEffectsToContainer="true" PanelClass="pa-6" Color="Color.Primary">
        <MudTabPanel Text="Evaluation Information" Icon="@Icons.Material.Filled.Assignment">
            <DisplayEvaluationInformation ModuleOfferingId="ModuleOfferingId" EvaluationId="EvaluationId"></DisplayEvaluationInformation>
        </MudTabPanel>
        <MudTabPanel Text="Add/Edit Marks" Icon="@Icons.Material.Filled.Edit">
            <AddEditEvaluationMarksTab ModuleOfferingId="ModuleOfferingId" EvaluationId="EvaluationId"></AddEditEvaluationMarksTab>
        </MudTabPanel>
        <MudTabPanel Text="View Marks and Final Marks" Icon="@Icons.Material.Filled.Pageview">
            <DisplayEvaluationResultList EvaluationId="EvaluationId"></DisplayEvaluationResultList>
        </MudTabPanel>
    </MudTabs>
</MudPaper>
@code {
    [Parameter] 
    public Guid ModuleOfferingId { get; set; }

    [Parameter] 
    public Guid EvaluationId { get; set; }
}